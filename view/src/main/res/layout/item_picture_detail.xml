<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/MatchMatch">

    <com.bili.diushoujuaner.widget.picture.PhotoView
        android:id="@+id/image_detail"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center" />

    <com.bili.diushoujuaner.widget.MaterialCircleView
        android:id="@+id/progress"
        android:layout_width="@dimen/x64"
        android:layout_height="@dimen/x64"
        android:layout_gravity="center"
        app:circleColor="@color/COLOR_WHITE"
        app:circleWidth="@dimen/x5" />

</FrameLayout>